    "extend": "^3.0.2",
    "fstream": "^1.0.12",
    "handlebars": "^4.3.0",
    "husky": "^1.0.0-rc.2",
    "i": "^0.3.6",
    "install": "^0.11.0",
    "is-float": "^1.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.13",
    "lodash.template": "^4.5.0",
    "mem": "^4.0.0",
    "merge": "^2.1.1",
    "mobx": "^4.2.0",
    "mobx-react": "5.0.0",
    "mobx-state-tree": "2.0.4",
    "money-currencies": "^2015.3.31",
    "morgan": "^1.9.1",
    "native-base": "^2.4.2",
